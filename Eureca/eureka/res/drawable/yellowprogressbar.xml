<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <gradient android:startColor="#ff000001" android:endColor="#ff0d1522" android:angle="270.0" android:centerY="0.75" android:centerColor="#ff0b131e" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient android:startColor="#fffcdd44" android:endColor="#fffcdd44" android:angle="270.0" android:centerY="0.75" android:centerColor="#fffcdd44" />
            </shape>
        </clip>
    </item>
</layer-list>